; 存档列表
id   bigint(20) auto primary comment="存档ID"
type  tinyint(1) comment="内容类型"
author bigint(20) key comment="发布人"
check int(1) key comment="审核状态"
ip varchar(32) key comment="发布人IP"
reply int(11) comment="回复"
view  int(11) comment="浏览"
create  int(11) comment="创建时间"
update  int(11) comment="最后更新"
allow_reply tinyint(1) default=1 comment="可回复"
data_type    bigint(20) key comment="储存对象类型"
data    bigint(20) key comment="储存对象"
state   tinyint(1)  key comment="存储状态"